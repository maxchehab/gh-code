{
  "name": "gh-code",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build:all": "yarn build:next && yarn build:ext",
    "build:ext": "tsc -p src/extension/tsconfig.json",
    "build:next": "next build src/next && next export src/next --outdir dist/next/out",
    "watch:ext": "tsc -p src/extension/tsconfig.json -w",
    "watch:next": "yarn watch 'yarn build:next' ./src/next"
  },
  "devDependencies": {
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "tslint": "5.19.0",
    "tslint-react": "4.0.0",
    "typescript": "3.6.2",
    "watch": "1.0.2",
    "web-ext-types": "3.2.1"
  },
  "dependencies": {
    "evergreen-ui": "4.18.3",
    "next": "9.3.2",
    "query-string": "6.8.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-extras": "0.9.0",
    "react-onclickout": "2.0.8",
    "ulid": "2.3.0"
  }
}
